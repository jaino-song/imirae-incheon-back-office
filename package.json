{
  "name": "imirae-incheon-back-office",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:frontend",
    "dev:frontend": "next dev apps/frontend",
    "build:frontend": "next build apps/frontend",
    "start:frontend": "next start apps/frontend",
    "dev:api": "nest start api --watch",
    "build:api": "nest build api",
    "build:shared-types": "nest build shared-types",
    "start:api": "node dist/apps/api/main.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,cts,mts,json,md,css,mjs,cjs}\"",
    "test:api": "jest --config apps/api/test/jest-e2e.json",
    "prepare": "husky install",
    "sync:lock": "node scripts/generate-package-lock.mjs"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,cts,mts,json,md,css,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/platform-express": "^11.0.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.15.0",
    "eslint-config-next": "16.0.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "supertest": "^7.0.0",
    "tailwindcss": "^4.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
